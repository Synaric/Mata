<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="SlideUpLayout">
        <attr name="handle" format="reference"/>
        <attr name="content" format="reference"/>
        <attr name="topOffset" format="dimension"/>
        <attr name="handleWidthOffset" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CompoundRecyclerView">
        <attr name="empty_view" format="reference"/>
        <attr name="error_view" format="reference"/>
        <attr name="content_view" format="reference" />
    </declare-styleable>
</resources>